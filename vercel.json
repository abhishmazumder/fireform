{
    "version": 2,
    "builds": [
        {
            "src": "apps/client/package.json",
            "use": "@vercel/static-build"
        },
        {
            "src": "apps/server/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/apps/server/src/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "outputDirectory": "apps/client/dist"
}