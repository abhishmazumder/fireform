{
    "name": "fireform",
    "version": "0.0.0",
    "scripts": {
        "client-dev": "npm --prefix client run dev",
        "server-dev": "npm --prefix server run dev",
        "dev": "concurrently \"npm run client-dev\" \"npm run server-dev\"",
        "build": "npm --prefix client run build",
        "client-start": "npm --prefix client run preview",
        "server-start": "npm --prefix server run start",
        "start": "concurrently \"npm run client-start\" \"npm run server-start\""
    },
    "devDependencies": {
        "concurrently": "^8.2.1"
    }
}